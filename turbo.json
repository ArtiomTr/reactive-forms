{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["prepared-package/**"]
		},
		"test": {
			"dependsOn": ["build"],
			"outputs": []
		},
		"lint": {
			"outputs": []
		},
		"dev": {
			"cache": false
		},
		"prepublishOnly": {
			"cache": false,
			"dependsOn": ["lint", "build"]
		},
		"postpublish": {
			"cache": false
		}
	}
}
