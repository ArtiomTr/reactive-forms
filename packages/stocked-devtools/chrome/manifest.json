{
    "manifest_version": 2,
    "name": "Stocked Developer Tools",
    "description": "Adds Stocked debugging tools to the Chrome Developer Tools.",
    "version": "0.0.1",
    "minimum_chrome_version": "60",
    "devtools_page": "panel.html",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["injectGlobalHook.js"],
            "run_at": "document_start"
        }
    ]
}
