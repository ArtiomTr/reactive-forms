    "homepage": "https://github.com/ArtiomTr/reactive-forms#readme",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/core.esm.js",
    "source": "src/index.ts",
    "types": "dist/core.d.ts",
    "files": [
        "dist"
    ],
    "publishConfig": {
