{
	"name": "@reactive-forms/core",
	"description": "Core package of Reactive Forms framework",
	"version": "0.8.12",
	"main": "dist/index.js",
	"module": "dist/core.esm.js",
	"types": "dist/core.d.ts",
	"bugs": "https://github.com/ArtiomTr/reactive-forms/issues",
	"homepage": "https://github.com/ArtiomTr/reactive-forms#readme",
	"repository": "ArtiomTr/reactive-forms.git",
	"author": "ReactiveForms team",
	"license": "MIT",
	"publishConfig": {
		"access": "public",
		"directory": "prepared-package"
	},
	"scripts": {
		"build": "aqu build",
		"dev": "aqu watch --no-cleanup",
		"lint": "eslint .",
		"lint:fix": "npm run lint --fix",
		"postpublish": "rimraf ./prepared-package",
		"prepublishOnly": "rimraf ./prepared-package && clean-publish",
		"test": "jest --passWithNoTests",
		"test:coverage": "jest --passWithNoTests --coverage --json --silent --ci --outputFile=\"report.json\"",
		"test:log-coverage": "jest --passWithNoTests --coverage --silent --ci --coverageReporters=text",
		"test:watch": "jest --passWithNoTests --watch"
	},
	"dependencies": {
		"lodash": "^4.17.19",
		"lodash-es": "^4.17.15",
		"pxth": "^0.6.0",
		"stocked": "^1.0.0-beta.28",
		"tiny-invariant": "^1.1.0"
	},
	"devDependencies": {
		"@babel/core": "^7.19.6",
		"@reactive-tools/eslint-config": "workspace:*",
		"@testing-library/react": "^13.4.0",
		"@types/jest": "^26.0.23",
		"@types/lodash": "^4.14.161",
		"@types/react": "18.0.23",
		"aqu": "^0.4.3",
		"clean-publish": "^4.0.1",
		"jest": "^29.2.2",
		"react": "18.2.0",
		"rimraf": "^3.0.2",
		"ts-jest": "^29.0.3",
		"tslib": "^2.3.1",
		"typescript": "^4.8.4",
		"yup": "^0.32.9"
	},
	"peerDependencies": {
		"react": ">=16"
	},
	"files": [
		"dist"
	],
	"source": "src/index.ts"
}
