{
    "name": "morfix",
    "version": "0.1.0",
    "description": "Complex forms with React? Easy!",
    "author": "Artiom Tretjakovas",
    "license": "MIT",
    "repository": "ArtiomTr/morfix",
    "main": "dist/index.js",
    "module": "dist/morfix.esm.js",
    "source": "src/index.ts",
    "types": "dist/index.d.ts",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "test:watch": "tsdx test --watch",
        "lint": "tsdx lint",
        "lint:fix": "tsdx lint --fix",
        "prepare": "tsdx build",
        "format:all": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
        "release": "np",
        "docs:gen": "node docs/tools/docgen/docgen.js --exclude \"src/typings/*\" --exclude \"src/utils/*\" --exclude \"src/**/index.ts\" --base src --tsconfig=./tsconfig.json --out=./docs/pages/api-reference src/**/*.ts src/**/*.tsx"
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "devDependencies": {
        "@types/lodash": "^4.14.161",
        "@types/react": "^16.9.49",
        "@types/react-dom": "^16.9.8",
        "@types/yup": "^0.29.7",
        "@typescript-eslint/eslint-plugin": "^4.1.1",
        "@typescript-eslint/parser": "^4.1.1",
        "eslint-plugin-simple-import-sort": "^5.0.3",
        "husky": "^4.3.0",
        "np": "^7.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "tsdx": "^0.14.1",
        "tslib": "^2.0.1",
        "typescript": "^4.0.3",
        "yup": "^0.29.3"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "lodash": "^4.17.19",
        "lodash-es": "^4.17.15",
        "stocked": "^0.2.0",
        "tiny-invariant": "^1.1.0"
    }
}
