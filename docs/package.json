{
    "name": "docs",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "export": "next export && echo \"This file tells github not to use jekyll.\nIt is required because jekyll not resolves underscore folders, like _next\" > out/.nojekyll",
        "push-gh-pages": "node scripts/publish.js",
        "deploy": "npm run build && npm run export && npm run push-gh-pages",
        "index-pages": "node scripts/indexPages.js"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@mdx-js/loader": "^1.6.18",
        "@mdx-js/react": "^1.6.18",
        "@next/mdx": "^9.5.3",
        "@types/classnames": "^2.2.10",
        "classnames": "^2.2.6",
        "gray-matter": "^4.0.2",
        "imagemin-svgo": "^8.0.0",
        "next": "9.5.3",
        "next-optimized-images": "^2.6.2",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "sass": "^1.26.11"
    },
    "devDependencies": {
        "gh-pages": "^3.1.0"
    }
}
